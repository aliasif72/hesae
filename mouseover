<body>
<h1 onclick="this.innerHTML='OOPS!'">click here</h1>
<div onmouseover="mover(this)" onmouseout="mout(this)"style="background-color:#D94A38; width:120px;height:20px;padding:60px;">Mouse over me</div>
<script>
function mover(obj)
{
obj.innerHTML="THank Your"
}
function mout(obj)
{
obj.innerHTML="CLICK ME"
}
</script>
</body>
</html>
